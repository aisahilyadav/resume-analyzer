{
    "version": 2,
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/app.js"
      }
    ],
    "env": {
      "MONGODB_URI": "@mongodb_uri",
      "JWT_SECRET": "@jwt_secret",
      "ENCRYPTION_KEY": "@encryption_key",
      "GEMINI_API_KEY": "@gemini_api_key"
    }
  }